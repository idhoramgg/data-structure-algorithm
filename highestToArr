var input = [
	[4, 5, 1, 3],
	[13, 27, 18, 26]];


function array(arr) {
	for (var i = 0; i < arr.length; i++) {
		var input = [0][0];
		for (var j = 0; j < arr[i].length; j++) {
			if (arr[i][j] > input) {
				input = arr[i][j];
			}
		}
	}

	return input;

}


console.log(array(input));

/*
  finding highest val from 2 array
*/
